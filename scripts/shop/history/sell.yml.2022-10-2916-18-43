#######################################################################
  'WHEAT':
    display:
      update: 20
      material: WHEAT
      lore:
      - '&b单价: &e 1.5'
      - "&b左键 &a➥ &c卖1组&e${js: 1.5  *  64}"
      - "&b右键 &a➥ &c卖1个&e${js: 1.5  *  1}"
      - "&b中键 &a➥ &c自定义"
      - "&bShift+左键 &a➥ &c卖10组&e${js: 1.5   *  640}"
      - ''
    actions:
      left:
        - condition: 'js: utils.hasItem(player, vars("mat:WHEAT,amt:64"))'
          actions:
          - 'take-item: material:WHEAT,amount:64'
          - 'give-money:  ${js: 64 * 1.5}'
          - 'tell: 您以$1.5的单价卖出了64个煤炭,收获${js: 64 * 1.5},当前余额%vault_eco_balance_fixed% 。'
          deny:
          - 'tell: &c您没有64个煤炭'
      right:
        - condition: 'js: utils.hasItem(player, vars("mat:WHEAT,amt:1"))'
          actions:
          - 'take-item: material:WHEAT,amount:1'
          - 'give-money:  ${js: 1 * 1.5}'
          - 'tell: 您以$1.5的单价卖出了1个煤炭,收获${js: 1 * 1.5},当前余额%vault_eco_balance_fixed% 。'
          deny:
          - 'tell: &c您没有1个煤炭'
      MIDDLE:
        - condition: 'js: utils.hasItem(player, vars("mat:WHEAT,amt:1"))'
          actions:
          - 'tell: 打开自定义菜单'
          - 'menu: Shop-Handler-Sell sell WHEAT 1.5 1'
          deny:
          - 'tell: &c您没有1个煤炭'
      shift_left:
        - condition: 'js: utils.hasItem(player, vars("mat:WHEAT,amt:640"))'
          actions:
          - 'take-item: material:WHEAT,amount:640'
          - 'give-money:  ${js: 640 * 1.5}'
          - 'tell: 您以$1.5的单价卖出了640个煤炭,收获${js: 640 * 1.5},当前余额%vault_eco_balance_fixed% 。'
          deny:
          - 'tell: &c您没有640个煤炭' 
